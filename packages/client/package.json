  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.4",
    "@types/lodash": "4.14.149",
    "@types/query-string": "6.3.0",
    "eslint-config-prettier": "6.10.1",
